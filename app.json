{
    "expo": {
        "name": "Shopfik",
        "slug": "Shopfik",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/shopfik.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/shopfik.png",
            "resizeMode": "contain",
            "backgroundColor": "#667eea"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/shopfik.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false
        },
        "web": {
            "favicon": "./assets/shopfik.png"
        },
        "plugins": ["expo-font"]
    }
}
